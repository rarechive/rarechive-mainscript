local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("My Hack Hub", "DarkTheme")

local Tab1 = Window:NewTab("Hacks")
local Section1 = Tab1:NewSection("Controls")
Section1:NewButton("Test Hack", "Chạy thử", function()
    print("Hack activated!")
end)
Section1:NewButton("Teleport to Spawn", "Dịch chuyển", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 10, 0)
end)
